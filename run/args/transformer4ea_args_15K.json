{
	"training_data": "../../datasets/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold",

  	"embedding_module": "Transformer4EA",
  	"alignment_module": "swapping",
	"search_module": "greedy",

	"hidden_size": 256,
	"num_layers": 8,
	"num_attention_heads": 8,
	"batch_size": 4096,
	"triple_batch_size": 512,
