	"alpha": 0.7,
	"beta": 0.7,

	"sim_th": 0.3,
	"k": 10,
	"is_bp": true,
	"start_bp": 1,
	"bp_w": 0.05,

	"num_samples": 20480,
	"max_epoch": 32,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 1,
	"eval_freq": 1,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": false,
	"eval_norm": true,

	"save_suffix": "nn",
	"start_inference": 2,
	"inverse_relation": false,
	"pos_thre": 0.6,
	"neg_thre": 0.1,
	"thre_h": 0.8,
	"thre_t": 0.8,
	"thre_re": 0.4
}

